# RagaSense-Data Environment Configuration
# Copy this file to .env and fill in your values

# Neo4j Database Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your_neo4j_password
NEO4J_DATABASE=ragasense

# Weights & Biases Configuration
WANDB_PROJECT=ragasense-data
WANDB_ENTITY=your_wandb_entity
WANDB_API_KEY=your_wandb_api_key
WANDB_MODE=online  # or "offline" for development

# Vector Database Configuration (Pinecone)
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=us-west1-gcp
PINECONE_INDEX_NAME=ragasense-audio-embeddings

# Kaggle API Configuration
KAGGLE_USERNAME=your_kaggle_username
KAGGLE_KEY=your_kaggle_api_key

# Data Processing Configuration
MAX_WORKERS=4
BATCH_SIZE=100
QUALITY_THRESHOLD=0.7
SCHEMA_COMPLIANCE_TARGET=0.95

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# Security Configuration
ENABLE_ENCRYPTION=true
AUDIT_LOGGING=true

# Development Configuration
DEBUG_MODE=false
TESTING_MODE=false
MOCK_EXTERNAL_APIS=false
